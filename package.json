{
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-structurize": "^2.3.2"
  },
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "prebuild": "rm -rf dist"
  },
  "parcel-plugin-structurize": {
    "rules": [
      {
        "match": "*.js",
        "folder": "scripts"
      }
    ],
    "verbose": true,
    "displayAssetsMap": true
  }
}
